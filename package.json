{
  "name": "floor-game",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build && node -e \"import { writeFileSync } from 'fs';const content='const { contextBridge, ipcRenderer } = require(\\\"electron\\\");contextBridge.exposeInMainWorld(\\\"electron\\\",{ ipcRenderer:{ send:(c,d)=>ipcRenderer.send(c,d), on:(c,f)=>ipcRenderer.on(c,(_,...a)=>f(...a)), off:(c,f)=>ipcRenderer.off(c,f) } });';writeFileSync('dist/preload.js',content);\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:app": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
    "build:app": "npm run build && electron-builder --mac"
  },
  "dependencies": {
    "phaser": "^3.90.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "build": {
    "appId": "com.thefloor.game",
    "productName": "The Floor",
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "mac": {
      "target": "dmg",
      "category": "public.app-category.games"
    },
    "asarUnpack": [
      "dist/preload.js"
    ]
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "concurrently": "^9.1.2",
    "electron": "^30.0.2",
    "electron-builder": "^25.1.8",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5",
    "wait-on": "^8.0.2"
  }
}
